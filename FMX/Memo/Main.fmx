              'begin'
              '  $3'
              'end;')
            InsertTextRules = itrInsertAsSnippet
            Preselect = False
          end>
        LibraryLocation = llOffline
        OnGetCodeCompletion = CodeCompletionMemoGetCodeCompletion
      end
    end
